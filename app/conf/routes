# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

# Rankings
PUT        /rankings/:ranking        com.plyrhub.ranking.controller.RankingController.createOrUpdate(ranking:String)

# Members
PUT        /members/:member          com.plyrhub.ranking.controller.MemberController.registrateMember(member:String)

GET        /assets/*file             controllers.Assets.at(path="/public", file)
