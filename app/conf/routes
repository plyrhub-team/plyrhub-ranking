# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
POST        /rankings/:ranking/view3        com.plyrhub.ranking.controller.RankingController.view3(ranking:String)
POST        /rankings/:ranking/view4        com.plyrhub.ranking.controller.RankingController.view4(ranking:String)
POST        /rankings/:ranking/view5        com.plyrhub.ranking.controller.RankingController.view5(ranking:String)
# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                   controllers.Assets.at(path="/public", file)
